{
    "name": "deegitalbe/server-authorization",
    "description": "Simple server authorization for communicating between our projects.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mathieu Henrotay",
            "email": "mathieu.henrotay@gmail.com"
        }
    ],
    "require": {
        "henrotaym/laravel-test-suite": "^2.0",
        "henrotaym/laravel-api-client": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Deegitalbe\\ServerAuthorization\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Deegitalbe\\ServerAuthorization\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Deegitalbe\\ServerAuthorization\\Providers\\ServerAuthorizationServiceProvider"
            ]
        }
    }
}
